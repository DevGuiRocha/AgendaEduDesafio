<p id="notice"><%= notice %></p>

<h1>Politicos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Sguf</th>
      <th>Sgpartido</th>
      <th>Id cadastro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @politicos.each do |politico| %>
      <tr>
        <td><%= politico.nome %></td>
        <td><%= politico.cpf %></td>
        <td><%= politico.sguf %></td>
        <td><%= politico.sgpartido %></td>
        <td><%= politico.id_cadastro %></td>
        <td><%= link_to 'Show', politico %></td>
        <td><%= link_to 'Edit', edit_politico_path(politico) %></td>
        <td><%= link_to 'Destroy', politico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
